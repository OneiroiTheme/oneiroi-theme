{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "red": "lightRed",
    "yellow": "lightYellow",
    "cyan": "lightCyan",
    "blue": "lightBlue",
    "white": "lightWhite",
    "1st": "p:red",
    "2nd": "p:yellow",
    "3rd": "p:cyan",
    "os": "p:3rd",
    "tag": "p:3rd",
    "type": "p:1st",
    "closer": "p:blue",
    "timer": "p:white"
  },
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "p:os",
          "style": "plain",
          "template": "{{ .Icon }} ",
          "type": "os"
        },
        {
          "foreground": "p:2nd",
          "style": "plain",
          "template": "{{ .UserName }}<p:1st>@</>{{ .HostName }}",
          "type": "session"
        },
        {
          "foreground": "p:tag",
          "style": "plain",
          "template": "[<p:type>\ue235{{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }}{{ end }}{{ end }}</>]",
          "type": "python"
        },
        {
          "foreground": "p:tag",
          "style": "plain",
          "properties": {
            "branch_icon": "\ue725",
            "cherry_pick_icon": "\ue29b",
            "commit_icon": "\uf417",
            "fetch_status": false,
            "fetch_upstream_icon": false,
            "merge_icon": "\ue727",
            "no_commits_icon": "\uf0c3",
            "rebase_icon": "\ue728",
            "revert_icon": "\uf0e2",
            "tag_icon": "\uf412"
          },
          "template": "[<p:type>{{ .HEAD }}</>]",
          "type": "git"
        },
        {
          "foreground": "p:1st",
          "properties": {
            "folder_icon": "\ue5fe",
            "home_icon": "~",
            "style": "agnoster_short"
          },
          "style": "plain",
          "template": " {{ .Path }} ",
          "type": "path"
        },
        {
          "foreground": "p:closer",
          "style": "plain",
          "template": "\uf105",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 2
}