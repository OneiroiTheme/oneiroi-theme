{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "1st": "p:red",
    "2nd": "p:yellow",
    "3rd": "p:cyan",
    "blue": "lightBlue",
    "closer": "p:blue",
    "cyan": "lightCyan",
    "os": "p:3rd",
    "red": "lightRed",
    "tag": "p:3rd",
    "timer": "p:white",
    "type": "p:1st",
    "white": "lightWhite",
    "yellow": "lightYellow"
  },
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "{{ .Icon }} ",
          "foreground": "p:os",
          "type": "os",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "{{ .UserName }}<p:1st>@</>{{ .HostName }}",
          "foreground": "p:2nd",
          "type": "session",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "home_enabled": true
          },
          "template": "[<p:type>\ue235{{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }}{{ end }}{{ end }}</>]",
          "foreground": "p:tag",
          "type": "python",
          "style": "plain"
        },
        {
          "properties": {
            "branch_icon": "\ue725",
            "cache_duration": "none",
            "cherry_pick_icon": "\ue29b",
            "commit_icon": "\uf417",
            "fetch_status": false,
            "fetch_upstream_icon": false,
            "merge_icon": "\ue727",
            "no_commits_icon": "\uf0c3",
            "rebase_icon": "\ue728",
            "revert_icon": "\uf0e2",
            "tag_icon": "\uf412"
          },
          "template": "[<p:type>{{ .HEAD }}</>]",
          "foreground": "p:tag",
          "type": "git",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "folder_icon": "\ue5fe",
            "home_icon": "~",
            "style": "agnoster_short"
          },
          "template": " {{ .Path }} ",
          "foreground": "p:1st",
          "type": "path",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "\uf105",
          "foreground": "p:closer",
          "type": "text",
          "style": "plain"
        }
      ]
    }
  ],
  "version": 3,
  "final_space": true
}
